#!/bin/python3

import math
import os
import random
import re
import sys

# Complete the extraLongFactorials function below.
def extraLongFactorials(n):
    Out = 1
    for x in range(1,n+1):
        
        Out = Out*x
    print( Out)

if __name__ == '__main__':
    n = int(input())

    extraLongFactorials(n)
